    "description": "The core component of the Slim Web Composer framework",
    "require": {
        "php": ">=7.1.0",
        "oomphinc/composer-installers-extender": "^1.1",
        "slim/slim": "^3.11",
        "slim/php-view": "^2.2",
        "monolog/monolog": "^1.17",
        "slim/twig-view": "^2.4",
        "juliangut/slim-controller": "^1.5",
        "slim/http-cache": "^0.4.0",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/cache": "^4.1",
        "oscarotero/psr7-middlewares": "^3.21",
        "willdurand/negotiation": "^2.3",
        "symfony/form": "^4.1",
        "symfony/twig-bridge": "^4.1",
        "symfony/security-csrf": "^4.1",
        "bryanjhv/slim-session": "^3.6",
        "firebase/php-jwt": "^5.0",
        "layershifter/tld-extract": "^2.0",
        "slim/flash": "^0.4.0",
        "dflydev/fig-cookies": "^1.0",
        "symfony/yaml": "^4.1",
        "symfony/asset": "^3.0",
